<!DOCTYPE html>
 <html class="no-js" lang="de">

    <head>
        {{> meta_data }}
        {{> app_icons }}
        {{> head_assets }}
    </head>
    <body>
        <div class="rb-page-wrapper">
            <header class="rb-page-header" role="banner">
                <div class="use-column-group use-gutter">
                    <div class="use-size-auto">
                        <h1>RawBlock</h1>
                    </div>
                </div>
            </header>

            <main class="rb-page-main" id="main-content" role="main" tabindex="-1">
                {{> body }}
            </main>

            <footer class="rb-page-footer">
                <div class="page-footer-inner">
                    <div class="use-size-100">
                        {{#with rb_sitemap }}
                            {{> rb_sitemap }}
                        {{/with}}
                    </div>
                </div>
                <div class="use-size-100">
                    {{#with rb_site_info }}
                        {{> rb_site_info }}
                    {{/with}}
                </div>
            </footer>

        </div>

        {{> body_assets}}
    </body>

</html>
